{
  "name": "backyard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "preinstall": "npx only-allow pnpm",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:local": "vite build --mode=local-dev",
    "build:watch": "vite build --mode=local-dev --watch",
    "preview": "vite preview",
    "codegen:local": "NODE_ENV=local-dev graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.7",
    "@urql/exchange-request-policy": "^1.2.1",
    "@urql/vue": "^1.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.11.0",
    "lucide-vue-next": "^0.511.0",
    "pinia": "^3.0.2",
    "reka-ui": "^2.3.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "vue": "^3.5.13",
    "vue-router": "4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@graphql-codegen/typescript-vue-urql": "^3.2.1",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.8"
  }
}
