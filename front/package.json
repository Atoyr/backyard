{
  "name": "backyard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "preinstall": "npx only-allow pnpm",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:local": "vite build --mode=local-dev",
    "build:watch": "vite build --mode=local-dev --watch",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@urql/exchange-request-policy": "^1.2.1",
    "@urql/vue": "^1.4.3",
    "graphql": "^16.11.0",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "4",
    "vuetify": "^3.8.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.8"
  }
}
